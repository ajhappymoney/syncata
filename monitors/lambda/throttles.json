{
	"name": "lambda.throttles: ${functionname}",
	"type": "metric alert",
	"query": "sum(last_1h):sum:aws.lambda.throttles{functionname:${functionname}} by {functionname}.as_count() > 100",
	"message": "lambda throttles monitor triggered for ${functionname}. Notify: ${notification_targets}",
	"options": {
		"notify_audit": false,
		"locked": false,
		"timeout_h": 0,
		"new_group_delay": 60,
		"require_full_window": false,
		"notify_no_data": false,
		"renotify_interval": "0",
		"evaluation_delay": 900,
		"thresholds": {
			"critical": 100
		}
	}
}