{
	"name": "dynamodb.read_throttle_events: ${tablename}",
	"type": "metric alert",
	"query": "sum(last_5m):sum:aws.dynamodb.read_throttle_events{tablename:${tablename}} by {tablename} > 0",
	"message": "dynamodb read throttle event monitor triggered for ${tablename}. Notify: ${notification_targets}",
	"options": {
		"notify_audit": false,
		"locked": false,
		"timeout_h": 0,
		"new_group_delay": 60,
		"require_full_window": false,
		"notify_no_data": false,
		"renotify_interval": "0",
		"evaluation_delay": 900,
		"thresholds": {
			"critical": 0
		}
	}
}